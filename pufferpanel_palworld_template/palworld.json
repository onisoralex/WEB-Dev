    "pre": [
      {
        "appId": "1007",
        "type": "steamgamedl"
      },
      {
        "appId": "2394010",
        "type": "steamgamedl"
      },
      {
        "commands": [
          "chmod +x PalServer.sh"
        ],
        "type": "command"
      }
    ],
    "post": [],
    "environmentVars": {}
